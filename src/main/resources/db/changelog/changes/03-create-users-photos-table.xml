<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.12.xsd">

    <!-- Create user table (H2-compatible) -->
    <changeSet id="3-create-user-photos-table" author="anoop">
        <createTable tableName="photos">
            <column name="id" type="BIGINT AUTO_INCREMENT">
                <constraints primaryKey="true" nullable="false" primaryKeyName="pk_photos"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="url" type="VARCHAR(200)">
                <constraints nullable="false"/>
            </column>
            <column name="created_at" type="TIMESTAMP" defaultValueComputed="CURRENT_TIMESTAMP"/>
        </createTable>
    </changeSet>

    <!-- Insert dummy data -->
    <changeSet id="2-insert-dummy-photos" author="anoop">
        <insert tableName="photos">
            <column name="id" value="1"/>
            <column name="user_id" value="1"/>
            <column name="url" value="url 1"/>
        </insert>
        <insert tableName="photos">
            <column name="id" value="2"/>
            <column name="user_id" value="1"/>
            <column name="url" value="url 2"/>
        </insert>
        <insert tableName="photos">
            <column name="id" value="3"/>
            <column name="user_id" value="2"/>
            <column name="url" value="url 3"/>
        </insert>
        <insert tableName="photos">
            <column name="id" value="4"/>
            <column name="user_id" value="2"/>
            <column name="url" value="url 4"/>
        </insert>
        <insert tableName="photos">
            <column name="id" value="5"/>
            <column name="user_id" value="3"/>
            <column name="url" value="url 5"/>
        </insert>
    </changeSet>
</databaseChangeLog>
